
public class Homework4 {
	public static void main(String[] args) {
		for(int i=0;i<10;i++){
			for(int j=9; j>i;j--){
				System.out.print(" ");
			}
			for(int g=0; g<i;g++){
				System.out.print("*");
				
			}
			for(int y=0; y<i;y++){
				System.out.println(" ");
			}
			
			System.out.println();
		}
	}

}
