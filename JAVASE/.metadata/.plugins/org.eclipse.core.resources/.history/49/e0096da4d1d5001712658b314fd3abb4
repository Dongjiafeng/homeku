package com.lanou3g.homework;

public class penguin {
  private String name;
  private int health = 100;
public String getName() {
	return name;
}
public void setName(String name) {
	this.name = name;
}
public int getHealth() {
	return health;
}
public void setHealth(int health) {
	this.health = health;
}
}
