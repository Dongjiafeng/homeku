package text;

public class Main {
	public static void main(String[] args){
		
		Student s = new Student();
//		s.name = "张三";
		//通过getName方法,
		//拿到s对象的name属性
		String name = s.getName();
		System.out.println(name);
		
		
		
		//得到sad 方法执行的结果
		//把这个结果赋值给了result这个变量
		String result = s.sad();
		//输出
		System.out.println(result);
	}
}
